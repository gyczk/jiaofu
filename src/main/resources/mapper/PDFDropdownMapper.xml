<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ryqg.jiaofu.web.mapper.PDFDropdownMapper">

    <select id="getDropdown" resultType="com.ryqg.jiaofu.domain.pojo.PDFDropdown">
        select id, parent_id, name, sort
        from sys_pdf_dropdowns
        order by sort
    </select>
</mapper>